<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision$ ($Author$) $Date$ -->
<module rename-to='portal'>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='gwtupload.GWTUpload' />
	
    <inherits name='com.propertyvista.portal.Rpc' />
    <inherits name='com.propertyvista.portal.Domain' />

    <inherits name='com.propertyvista.common.VistaCommon' />
    
    <inherits name='com.pyx4j.gwt.GwtGeo' />
    <inherits name="com.google.gwt.maps.GoogleMaps" />

    <source path="client"/>

    <entry-point class='com.propertyvista.portal.client.PortalSite'/>

    <extend-configuration-property name="rpc.blacklist" value="com.propertyvista.portal.client.*" />

</module>