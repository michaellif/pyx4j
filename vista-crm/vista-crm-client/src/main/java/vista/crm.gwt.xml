<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision$ ($Author$) $Date$ -->
<module rename-to="crm">
    <inherits name='com.propertyvista.crm.Client'/>

    <extend-property name="locale" values="en_US"/>

    <extend-property name="locale" values="ru"/>
    <extend-property name="locale" values="fr"/>
    <extend-property name="locale" values="es"/>
    <extend-property name="locale" values="zh_CN"/>
    <extend-property name="locale" values="zh_TW"/>

    <set-property-fallback name="locale" value="en_US"/>
    <set-property name="locale" value="en_US,ru,fr,es,zh_CN,zh_TW"/>

    <set-configuration-property name="locale.cookie" value="locale"/>
	<set-configuration-property name="pyx.locale.default" value="en_US"/>

    <set-property name="user.agent" value="opera,safari,gecko1_8,ie8,ie9"/>
    
    <inherits name='com.google.gwt.user.Debug'/>
</module>