	<inherits name='com.propertyvista.common.VistaCommon' />

    <source path="client"/>

    <entry-point class='com.propertyvista.crm.client.CrmSite'/>
    
    <extend-configuration-property name="rpc.blacklist" value="com.propertyvista.crm.client.*" />
    
</module>