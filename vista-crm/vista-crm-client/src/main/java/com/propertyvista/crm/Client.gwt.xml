<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision$ ($Author$) $Date$ -->
<module rename-to='crm'>
	
	<inherits name='com.google.gwt.user.User'/>

    <inherits name='com.pyx4j.site.GwtSite' />

    <inherits name='com.propertyvista.Domain' />    
    <inherits name='com.propertyvista.crm.Rpc' />
    <inherits name='com.propertyvista.crm.SVG' />

    <inherits name='com.pyx4j.essentials.EssentialsSwf' />
    
	<inherits name='com.propertyvista.common.VistaCommon' />

    <source path="client"/>    

    <entry-point class='com.propertyvista.crm.client.CrmSite'/>
    
    <extend-configuration-property name="rpc.blacklist" value="com.propertyvista.crm.client.*" />
    
    <generate-with class="com.propertyvista.crm.rebind.GadgetFactoryGenerator">
    	<when-type-is class="com.propertyvista.crm.client.ui.gadgets.common.IGadgetFactory"/>
  	</generate-with>
    
    <set-property name="user.agent" value="safari,gecko1_8,ie8,ie9,ie10"/>
</module>