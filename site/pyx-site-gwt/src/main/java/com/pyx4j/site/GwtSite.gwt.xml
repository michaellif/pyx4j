<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision: 8012 $ ($Author: michaellif $) $Date: 2011-02-05 09:46:46 -0500 (Sat, 05 Feb 2011) $ -->
<module>
    <inherits name='com.google.gwt.user.User'/>
	<inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
	<inherits name="com.google.gwt.inject.Inject"/>
    <inherits name='com.pyx4j.log4gwt.Log4gwt'/>
    <inherits name='com.pyx4j.forms.Forms'/>
	<inherits name='com.pyx4j.entity.Entity' />
    <inherits name='com.pyx4j.site.SiteShared'/>
    <inherits name='com.pyx4j.security.Security'/>
    <inherits name='com.pyx4j.webstorage.HTML5LocalStorage'/>

    <generate-with class="com.pyx4j.site.rebind.AppPlaceListingGenerator">
        <when-type-assignable class="com.pyx4j.site.client.place.AppPlaceListing"/>
    </generate-with>
  
	<source path='client' />
</module>
