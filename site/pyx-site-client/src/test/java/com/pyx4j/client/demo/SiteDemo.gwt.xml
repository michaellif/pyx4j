<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='test'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.user.Debug"/>
	<inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
	<inherits name="com.google.gwt.inject.Inject"/>

	<inherits name='com.pyx4j.site.Site' />

	<entry-point class='com.pyx4j.client.demo.client.SiteDemo' />

	<!-- Use ClientFactoryImpl by default -->
  	<replace-with class="com.pyx4j.client.demo.client.place.AppPlaceHistoryMapper">
    	<when-type-is class="com.google.gwt.place.shared.PlaceHistoryMapper"/>
  	</replace-with>
  
      <generate-with class="com.pyx4j.client.demo.rebind.AppPlaceListingGenerator">
        <when-type-assignable class="com.pyx4j.client.demo.client.place.AppPlaceListing"/>
    </generate-with>
  
	<source path='client' />
</module>
