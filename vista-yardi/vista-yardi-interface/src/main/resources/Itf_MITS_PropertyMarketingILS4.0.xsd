<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2011 rel. 3 sp1 (x64) (http://www.altova.com) by ShawnJ (Kroll Factual Data) -->
<!-- MITS Property XML Standard v4.0 -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:MITS="http://my-company.com/namespace" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="./Itf_MITS_CustomExtension.xsd"/>
	<xs:include schemaLocation="./Itf_MITS_CoreData4.0.xsd"/>
	<xs:element name="PhysicalProperty">
		<xs:annotation>
			<xs:documentation>MITS Property v4.0 Initial Draft 04/2/2011</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Management" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="Identification">
								<xs:sequence>
									<xs:element name="PropertyContacts" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Node contians CompanyType and PersonType</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Companies" type="CompanyType" minOccurs="0" maxOccurs="unbounded"/>
												<xs:element name="Persons" type="PersonType" minOccurs="0" maxOccurs="unbounded"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="File" type="FileType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>FileID, Active</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Property" maxOccurs="unbounded">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="Identification">
								<xs:sequence>
									<xs:element name="PropertyID">
										<xs:annotation>
											<xs:documentation>MITS 4.0 PropertyType</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="PropertyType"/>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="General_ID" type="Identification" minOccurs="0" maxOccurs="10"/>
									<xs:element name="ILS_Identification">
										<xs:annotation>
											<xs:documentation>ILS_IdentificationType, RentalType</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Latitude" type="xs:decimal" minOccurs="0"/>
												<xs:element name="Longitude" type="xs:decimal" minOccurs="0"/>
												<xs:element name="DaylightSaving" type="xs:boolean" minOccurs="0"/>
												<xs:element name="TimeZone" minOccurs="0">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="100"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="LastUpdate" minOccurs="0">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="ILS_IdentificationType" use="required">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="Apartment"/>
														<xs:enumeration value="Condo"/>
														<xs:enumeration value="Duplex"/>
														<xs:enumeration value="Triplex"/>
														<xs:enumeration value="4plex"/>
														<xs:enumeration value="Senior"/>
														<xs:enumeration value="Assisted Living"/>
														<xs:enumeration value="Subsidized"/>
														<xs:enumeration value="High Rise"/>
														<xs:enumeration value="Garden Style"/>
														<xs:enumeration value="Mid Rise"/>
														<xs:enumeration value="Mixed Use"/>
														<xs:enumeration value="Townhouse"/>
														<xs:enumeration value="Vacation"/>
														<xs:enumeration value="Campus"/>
														<xs:enumeration value="Military"/>
														<xs:enumeration value="House for Rent"/>
														<xs:enumeration value="Corporate"/>
														<xs:enumeration value="Unspecified"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="RentalType" use="required">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="Subsidized"/>
														<xs:enumeration value="Assisted Senior"/>
														<xs:enumeration value="Assisted Living"/>
														<xs:enumeration value="Market Rate"/>
														<xs:enumeration value="Unspecified"/>
														<xs:enumeration value="Broker"/>
														<xs:enumeration value="Student"/>
														<xs:enumeration value="Temporary"/>
														<!-- Added 4/12/2011 -->
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
									<xs:element name="Information">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="StructureType">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Standard"/>
															<xs:enumeration value="High Rise"/>
															<xs:enumeration value="Garden Style"/>
															<xs:enumeration value="Mid Rise"/>
															<xs:enumeration value="Mixed Use"/>
															<xs:enumeration value="Unspecified"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="BuildingCount" type="xs:integer" minOccurs="0"/>
												<xs:element name="UnitCount" type="xs:integer"/>
												<xs:element name="NumberOfAcres" type="xs:decimal" minOccurs="0"/>
												<xs:element name="YearBuilt" type="xs:gYear" minOccurs="0"/>
												<xs:element name="YearRemodeled" type="xs:gYear" minOccurs="0"/>
												<xs:element name="AcquisitionDate" minOccurs="0">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="OfficeHour" minOccurs="0" maxOccurs="unbounded">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="OpenTime">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="50"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="CloseTime">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="50"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="Day">
																<xs:annotation>
																	<xs:documentation>Days open: Su, M, T, W, Th, F, Sa </xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="20"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="ShortDescription" minOccurs="0">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="500"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="LongDescription" minOccurs="0">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="LeaseLength" minOccurs="0">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="100"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Rents" minOccurs="0">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="SalePrice" type="xs:decimal" minOccurs="0"/>
															<xs:element name="StartRent" type="xs:decimal" minOccurs="0"/>
															<xs:element name="StandardRent" type="xs:decimal" minOccurs="0"/>
															<xs:element name="CorporateRent" minOccurs="0">
																<xs:complexType>
																	<xs:attribute name="Min" type="xs:decimal" use="optional"/>
																	<xs:attribute name="Max" type="xs:decimal" use="optional"/>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="IncomeRentQual" minOccurs="0" maxOccurs="6">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="IncomeLimit" type="xs:decimal" minOccurs="0"/>
															<xs:element name="QualifiedRent" type="xs:decimal" minOccurs="0"/>
														</xs:sequence>
														<xs:attribute name="Persons" use="required">
															<xs:simpleType>
																<xs:restriction base="xs:integer">
																	<xs:enumeration value="1"/>
																	<xs:enumeration value="2"/>
																	<xs:enumeration value="3"/>
																	<xs:enumeration value="4"/>
																	<xs:enumeration value="5"/>
																	<xs:enumeration value="6"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
												<xs:element name="Parking" minOccurs="0" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>Type</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Assigned" type="xs:boolean" minOccurs="0"/>
															<xs:element name="AssignedFee" minOccurs="0">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:enumeration value="Free"/>
																		<xs:enumeration value="Paid"/>
																		<xs:enumeration value="Both"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="SpaceFee" type="xs:decimal" minOccurs="0"/>
															<xs:element name="Spaces" type="xs:integer" minOccurs="0"/>
															<xs:element name="Comment" minOccurs="0">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="500"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
														</xs:sequence>
														<xs:attribute name="ParkingType" use="required">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="Surface Lot"/>
																	<xs:enumeration value="Garage Lot"/>
																	<xs:enumeration value="Covered Lot"/>
																	<xs:enumeration value="Street"/>
																	<xs:enumeration value="Other"/>
																	<xs:enumeration value="None"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
												<xs:element name="DrivingDirections" minOccurs="0">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Services" minOccurs="0" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>Nearest</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Name">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="200"/>
																		<xs:enumeration value="Bus"/>
																		<xs:enumeration value="Elementary School"/>
																		<xs:enumeration value="Middle School"/>
																		<xs:enumeration value="High School"/>
																		<xs:enumeration value="Highway"/>
																		<xs:enumeration value="Laundromat"/>
																		<xs:enumeration value="Drycleaner"/>
																		<xs:enumeration value="Park"/>
																		<xs:enumeration value="Train"/>
																		<xs:enumeration value="University"/>
																		<xs:enumeration value="Other"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="Detail" minOccurs="0">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="500"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="DistanceTo" minOccurs="0">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="100"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="Comment" minOccurs="0">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="500"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
														</xs:sequence>
														<xs:attribute name="Nearest" type="xs:boolean" use="required"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="PropertyAvailabilityURL" minOccurs="0">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="250"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Accounting" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="LiveDate">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="LiveFlag" type="xs:boolean"/>
												<xs:element name="LastDayBegin">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="LastDayEnd">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="LastAccMthEnd">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="AcctMthEndFlag">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Preliminary"/>
															<xs:enumeration value="Changes"/>
															<xs:enumeration value="Month Closed"/>
															<xs:enumeration value="None"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="LastInvoiceNumber" default="0">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="40"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="LastReceiptNumber">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="40"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="CashReserve" type="xs:decimal"/>
												<xs:element name="EndOfYear">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="AccountsReceivable">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="AccountsPayable">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="GeneralLedger">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="CurrentAccountingPeriod">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="CurrentAccountingPeriodAR">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="CurrentAccountingPeriodAP">
													<xs:complexType>
														<xs:attributeGroup ref="MITS-DateGroup"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="TaxCredit">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="100"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Payment" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="RemitTo" type="xs:string"/>
												<xs:element name="CheckPayable">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="100"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Method">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Cash"/>
															<xs:enumeration value="Check"/>
															<xs:enumeration value="CreditCard"/>
															<xs:enumeration value="Insurance"/>
															<xs:enumeration value="CorporateBilling"/>
															<xs:enumeration value="PerDiem"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Fee" minOccurs="0">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="FeeType">
													<xs:sequence>
														<xs:element name="AdminFee" type="xs:decimal"/>
														<xs:element name="ApplicationFee" type="xs:decimal"/>
														<!-- Added in ILS 4.0 -->
														<xs:element name="BrokerFee" type="xs:decimal"/>
													</xs:sequence>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="Concession" minOccurs="0" maxOccurs="10">
										<xs:annotation>
											<xs:documentation>Active</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="ConcessionType"/>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="Amenity" type="GeneralAmenityType" minOccurs="0" maxOccurs="unbounded"/>
									<xs:element name="Deposit" type="DepositType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Type</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Policy" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Pet" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>Allowed</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Pets" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Details about various prospect pets</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:complexContent>
																		<xs:extension base="PetType"/>
																	</xs:complexContent>
																</xs:complexType>
															</xs:element>
															<xs:element name="Comment" minOccurs="0">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="500"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="Deposit" type="xs:decimal" minOccurs="0"/>
															<xs:element name="Fee" type="xs:decimal" minOccurs="0"/>
															<xs:element name="Rent" type="xs:decimal" minOccurs="0"/>
															<xs:element name="FeeRange" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Min, Max </xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:attribute name="Min" type="xs:decimal" use="optional"/>
																	<xs:attribute name="Max" type="xs:decimal" use="optional"/>
																</xs:complexType>
															</xs:element>
															<xs:element name="Restrictions" minOccurs="0">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="500"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="PetCare" type="xs:boolean" minOccurs="0"/>
														</xs:sequence>
														<xs:attribute name="Allowed" type="xs:boolean" use="required"/>
													</xs:complexType>
												</xs:element>
												<xs:element name="General" minOccurs="0">
													<xs:annotation>
														<xs:documentation>General Policy</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="500"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Phase" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="Identification">
													<xs:sequence>
														<xs:element name="Name">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:maxLength value="100"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
														<xs:element name="Description">
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:maxLength value="1000"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
														<xs:element name="UnitCount" type="xs:integer"/>
														<xs:element name="RentableUnits" type="xs:integer"/>
