  <wsdl:portType name="ItfILSGuestCard2_0Soap">
    <wsdl:operation name="UnitAvailability_Login">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for the export of unit availibility for a given property/property list. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:UnitAvailability_LoginSoapIn" />
      <wsdl:output message="tns:UnitAvailability_LoginSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetYardiAgentsSourcesResults_Login">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for the export of agent names, sources, and result values; which are required to enter a guest. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetYardiAgentsSourcesResults_LoginSoapIn" />
      <wsdl:output message="tns:GetYardiAgentsSourcesResults_LoginSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetYardiGuestActivity_Login">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for the export of guest activity and is subject to the optional parameter in vendor configuration which specifies the last number of days to be included. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetYardiGuestActivity_LoginSoapIn" />
      <wsdl:output message="tns:GetYardiGuestActivity_LoginSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetYardiGuestActivity_DateRange">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for the export of guest activity.  The FROM and TO dates will pull all guests with documented activity within the given period. This does not necessarily mean all events will be exported; only major events such as Show, Lease Signed, Move In events will be exported. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetYardiGuestActivity_DateRangeSoapIn" />
      <wsdl:output message="tns:GetYardiGuestActivity_DateRangeSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetYardiGuestActivity_Search">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for the export of guest activity. This method will allow the consumer enter search critera based on 1)First name 2)Last name 3)E-mail Address 4)Phone number 5)Third Party ID/Prospect Code. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetYardiGuestActivity_SearchSoapIn" />
      <wsdl:output message="tns:GetYardiGuestActivity_SearchSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetRecentYardiGuestActivity">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for the export of guest activity.  This method will export all activity since the last export for this vendor/property combination. This does not necessarily mean all events will be exported; only major events such as Show, Lease Signed, Move In events will be exported. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetRecentYardiGuestActivitySoapIn" />
      <wsdl:output message="tns:GetRecentYardiGuestActivitySoapOut" />
    </wsdl:operation>
    <wsdl:operation name="ImportYardiGuest_Login">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for Login and the import of guests into a Yardi Voyager database. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:ImportYardiGuest_LoginSoapIn" />
      <wsdl:output message="tns:ImportYardiGuest_LoginSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetPropertyConfigurations">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows export of the Property Configuration with the Database. The Unique Interface Entity name is needed in order to return the Property ID(s) the Third-Party has access to. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetPropertyConfigurationsSoapIn" />
      <wsdl:output message="tns:GetPropertyConfigurationsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="ImportApplication_Login">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for Login and update of a prospect's application form and leasing specials/application fees into a Yardi Voyager database. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:ImportApplication_LoginSoapIn" />
      <wsdl:output message="tns:ImportApplication_LoginSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetApplication_Login">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows for export of prospect's application form from Yardi Voyager database. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetApplication_LoginSoapIn" />
      <wsdl:output message="tns:GetApplication_LoginSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetVersionNumber">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Returns current version number of Interfaces concatenated with ILS/GuestCard Plugin version (XX_Y)</documentation>
      <wsdl:input message="tns:GetVersionNumberSoapIn" />
      <wsdl:output message="tns:GetVersionNumberSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="Ping">
      <wsdl:input message="tns:PingSoapIn" />
      <wsdl:output message="tns:PingSoapOut" />
