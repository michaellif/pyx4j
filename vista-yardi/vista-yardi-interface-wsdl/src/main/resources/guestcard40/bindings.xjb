<jxb:bindings version="2.0"
    xmlns:jxb="http://java.sun.com/xml/ns/jaxb" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:namespace="http://jaxb2-commons.dev.java.net/namespace-prefix">

    <jxb:globalBindings localScoping="toplevel">
	<jxb:javaType name="java.lang.Integer" xmlType="xs:integer" parseMethod="javax.xml.bind.DatatypeConverter.parseInt" printMethod="javax.xml.bind.DatatypeConverter.printInt"/>
	<jxb:javaType name="java.util.Date" xmlType="xs:date" parseMethod="com.propertyvista.yardi.xml.bind.DateAdapter.parseDate" printMethod="com.propertyvista.yardi.xml.bind.DateAdapter.printDate"/>
	<jxb:javaType name="java.sql.Timestamp" xmlType="xs:dateTime" parseMethod="com.propertyvista.yardi.xml.bind.DateAdapter.parseDateTime" printMethod="com.propertyvista.yardi.xml.bind.DateAdapter.printDateTime"/>

	<jxb:serializable uid="1" />
    </jxb:globalBindings>

    <jxb:bindings schemaLocation="Itf_MITS_CoreData4.0.xsd" >
        <jxb:schemaBindings>
            <jxb:package name="com.yardi.entity.guestcard40"/>
        </jxb:schemaBindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="Itf_MITS_LeadManagement4.0.xsd" >
    </jxb:bindings>

    <jxb:bindings schemaLocation="YardiAgentsSources.xsd" >
	<jxb:bindings node="//xsd:element[@name='Properties']">
            <jxb:class name="MarketingSources"/>
        </jxb:bindings>
	<jxb:bindings node="//xsd:complexType[@name='Property']">
            <jxb:class name="PropertyMarketingSources"/>
        </jxb:bindings>
	<jxb:bindings node="//xsd:complexType[@name='Agents']/xsd:sequence/xsd:element[@name='AgentName']/xsd:complexType">
            <jxb:class name="MarketingAgent"/>
        </jxb:bindings>
	<jxb:bindings node="//xsd:complexType[@name='Sources']/xsd:sequence/xsd:element[@name='SourceName']/xsd:complexType">
            <jxb:class name="MarketingSource"/>
        </jxb:bindings>
	<jxb:bindings node="//xsd:complexType[@name='Results']/xsd:sequence/xsd:element[@name='Result']/xsd:complexType">
            <jxb:class name="MarketingResult"/>
        </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="YardiRentableItems.xsd" >
	<jxb:bindings node="//xsd:element[@name='ItemType']/xsd:complexType">
            <jxb:class name="RentableItemType"/>
        </jxb:bindings>
	<jxb:bindings node="//xsd:element[@name='Item']/xsd:complexType">
            <jxb:class name="RentableItem"/>
        </jxb:bindings>
    </jxb:bindings>

</jxb:bindings>
