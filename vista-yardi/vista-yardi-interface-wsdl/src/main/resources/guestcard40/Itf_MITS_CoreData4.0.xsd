	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation>
				<p>This element describes a (real-world or e-mail) address</p>
				<dl>
					<dt>Description (optional)</dt>
					<dd>String description for Other Address Type</dd>
					<dt>AddressLine1 (optional)</dt>
					<dd>PO Box or Street number, direction, street name, suffix</dd>
					<dt>AddressLine2 (optional)</dt>
					<dd>Additional address line</dd>
					<dt>City  (optional)</dt>
					<dd>City property is located in</dd>
					<dt>State  (optional)</dt>
					<dd>The State attribute describes the 2-3 character state code of the
		            address, not the full state name.</dd>
					<dt>Province  (optional)</dt>
					<dd>Province if not US property</dd>
					<dt>Postal Code   (optional)</dt>
					<dd>Property postal code</dd>
					<dt>Country   (optional)</dt>
					<dd>Country property is located in</dd>
					<dt>CountryName    (optional)</dt>
					<dd>County common name</dd>
					<dt>UnparsedAddress    (optional)</dt>
				</dl>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Description" type="StringMax100Type" minOccurs="0"/>
			<xs:element name="AddressLine1" type="StringMax100Type" minOccurs="0"/>
			<xs:element name="AddressLine2" type="StringMax100Type" minOccurs="0"/>
			<xs:element name="City" type="StringMax60Type" minOccurs="0"/>
			<xs:element name="State" type="StringMax3Type" minOccurs="0"/>
			<xs:element name="Province" type="StringMax60Type" minOccurs="0"/>
			<xs:element name="PostalCode" type="StringMax12Type" minOccurs="0"/>
			<xs:element name="Country" type="StringMax50Type" minOccurs="0"/>
			<xs:element name="CountyName" type="StringMax60Type" minOccurs="0"/>
			<xs:element name="UnparsedAddress" type="StringMax500Type" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="AddressType" type="AddressInfo" use="required"/>
	</xs:complexType>
