					<xs:element name="MaxSquareFeet" type="Integer5Digit" minOccurs="0"/>
					<xs:element name="SquareFootType" type="UnitSqftInfo" minOccurs="0"/>
					<xs:element name="UnitRent" type="Decimal7Digits2Fraction" minOccurs="0"/>
					<xs:element name="MarketRent" type="Decimal7Digits2Fraction" minOccurs="0"/>
					<xs:element name="UnitEconomicStatus" type="UnitEconStatusInfo" minOccurs="0"/>
					<xs:element name="UnitEconomicStatusDescription" type="StringMax100Type" minOccurs="0"/>
					<xs:element name="UnitOccupancyStatus" type="UnitOccpStatusInfo" minOccurs="0"/>
					<xs:element name="UnitLeasedStatus" type="UnitLeaseStatusInfo" minOccurs="0"/>
					<xs:element name="UnitLeasedStatusDescription" type="StringMax100Type" minOccurs="0"/>
					<xs:element name="NumberOccupants" type="CurrentNumberOccupantsType" minOccurs="0"/>
					<xs:element name="FloorplanName" type="StringMax50Type" minOccurs="0"/>
					<xs:element name="PhaseName" type="StringMax50Type" minOccurs="0"/>
					<xs:element name="BuildingName" type="StringMax50Type" minOccurs="0"/>
					<xs:element name="Address" type="AddressType" minOccurs="0" maxOccurs="10"/>
