	<xs:complexType name="CurrencyRangeType">
		<xs:annotation>
			<xs:documentation>
				<p>A Range Of Currencies - pattern based
		
		The Min and Max attributes specify the minimum and maximum acceptable
		amounts.  Alternatively, the Exact attribute can be used to specify an
		exact amount.</p>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Min" type="Money" use="optional"/>
		<xs:attribute name="Max" type="Money" use="optional"/>
		<xs:attribute name="Exact" type="Money" use="optional"/>
		<xs:attribute name="Currency" type="Currency" use="optional"/>
	</xs:complexType>
