					<dt>ExpectedMoveInDate</dt>
					<dd>Expected Move In date</dd>
					<dt>ExpectedMoveOutDate</dt>
					<dd>Expected Move Out date</dd>
					<dt>ActualMoveIn</dt>
					<dd>Actual Move In date</dd>
					<dt>ActualMoveOut</dt>
					<dd>Actual Move Out date</dd>
					<dt>ResponsibleForLease</dt>
					<dd>Respnsible for Lease? (Y/N)</dd>
					<dt>LeaseSignDate</dt>
					<dd>Date Lease signed</dd>
					<dt>SpecialStatus</dt>
					<dd>Special status</dd>
					<dt>PaymentAccepted</dt>
					<dd>Payment accepted</dd>
					<dt>AccountNumber</dt>
					<dd>Account Number</dd>
					<dt>CurrentNumberOccupants</dt>
					<dd>Total number of Occupants, with number of children optional</dd>
				</dl>
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Identifiable">
				<xs:sequence>
					<xs:element name="CurrentRent" type="xs:decimal" minOccurs="0"/>
					<xs:element name="ExpectedMoveInDate" type="xs:date" minOccurs="0"/>
					<xs:element name="ExpectedMoveOutDate" type="xs:date" minOccurs="0"/>
					<xs:element name="LeaseFromDate" type="xs:date" minOccurs="0"/>
					<xs:element name="LeaseToDate" type="xs:date" minOccurs="0"/>
					<xs:element name="ActualMoveIn" type="xs:date" minOccurs="0"/>
					<xs:element name="ActualMoveOut" type="xs:date" minOccurs="0"/>
					<xs:element name="ResponsibleForLease" type="xs:boolean" minOccurs="0"/>
					<xs:element name="LeaseSignDate" type="xs:date" minOccurs="0"/>
					<xs:element name="SpecialStatus" type="StringMax50Type" minOccurs="0"/>
					<xs:element name="PaymentAccepted" type="StringMax50Type" minOccurs="0"/>
					<xs:element name="AccountNumber" type="StringMax50Type" minOccurs="0"/>
					<xs:element name="CurrentNumberOccupants" type="CurrentNumberOccupantsType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
