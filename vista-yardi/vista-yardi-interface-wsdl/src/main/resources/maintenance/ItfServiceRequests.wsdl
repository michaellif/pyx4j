  <wsdl:portType name="ItfServiceRequestsSoap">
    <wsdl:operation name="GetPropertyConfigurations">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Allows the export of a list of properties from the Yardi Voyager database that are associated with an agency for Service Requests. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetPropertyConfigurationsSoapIn" />
      <wsdl:output message="tns:GetPropertyConfigurationsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetResident_Search">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Searches for residents given the search criteria determined by the web method parameters. This method will allows a search for a resident based on any of the following: 1)Unit Code 2)Resident Code 3)Resident Name 4)Phone number 5)Address. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetResident_SearchSoapIn" />
      <wsdl:output message="tns:GetResident_SearchSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetServiceRequest_Search">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Searches for work orders given the search criteria determined by the web method parameters. This method will allow a service request search based on any of the following: 1)Unit Code 2)Resident Code 3)Resident Name 4)Unit Address 5)Resident Phone Number 6)Current Status 7)Open Or Closed 8)Date Last Modified From Date(MM/DD/YYYY) 9)Date Last Modified To Date(MM/DD/YYYY). Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetServiceRequest_SearchSoapIn" />
      <wsdl:output message="tns:GetServiceRequest_SearchSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="CreateOrEditServiceRequests">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Accepts an xml file of work order information, creates or updates work orders based on the work order ID, and returns information on those work orders in the response xml. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:CreateOrEditServiceRequestsSoapIn" />
      <wsdl:output message="tns:CreateOrEditServiceRequestsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetCustomValues">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Returns client specified custom values in the response xml. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetCustomValuesSoapIn" />
      <wsdl:output message="tns:GetCustomValuesSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetServiceRequests">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Accepts an array of WorkOrderIds (seperated by commas) and returns information regarding those work orders in the response xml. Can be used thru 1/1/13 without an InterfaceLicense.</documentation>
      <wsdl:input message="tns:GetServiceRequestsSoapIn" />
      <wsdl:output message="tns:GetServiceRequestsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetVersionNumber">
      <documentation xmlns="http://schemas.xmlsoap.org/wsdl/">Returns current version number of Interfaces concatenated with Service Requests Plugin version (XX_Y)</documentation>
      <wsdl:input message="tns:GetVersionNumberSoapIn" />
      <wsdl:output message="tns:GetVersionNumberSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="Ping">
      <wsdl:input message="tns:PingSoapIn" />
      <wsdl:output message="tns:PingSoapOut" />
    </wsdl:operation>
