<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module>
    <inherits name='com.pyx4j.config.ConfigShared'/>
    <inherits name='com.pyx4j.entity.EntityShared'/>
    <inherits name='com.pyx4j.rpc.RPCShared'/>
    <source path="rpc"/>
    <source path="shared"/>
    
    <extend-configuration-property name="rpc.blacklist" value="com.pyx4j.essentials.rpc.admin.BackupEntityProperty" />
    <extend-configuration-property name="rpc.blacklist" value="com.pyx4j.essentials.rpc.admin.BackupKey" />
    <extend-configuration-property name="rpc.blacklist" value="com.pyx4j.essentials.rpc.admin.BackupRecordsResponse" />
    <extend-configuration-property name="rpc.blacklist" value="com.pyx4j.essentials.rpc.admin.BackupRequest" />
    
    <!-- for now we define site only on client -->
    <extend-configuration-property name="rpc.blacklist" value="com.pyx4j.site.shared.domain.*" />
</module>