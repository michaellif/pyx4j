<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module>
    <inherits name='com.google.gwt.user.User'/>
	<inherits name='com.pyx4j.Commons'/>
    <inherits name='com.pyx4j.JUnitCategories'/>

	<inherits name='com.pyx4j.widgets.Widgets'/>

	<inherits name='com.pyx4j.unit.UnitShared'/>

    <generate-with class="com.pyx4j.unit.rebind.GUnitMetaDataGenerator">
        <when-type-assignable class="com.pyx4j.unit.client.impl.TestSuiteMetaData"/>
    </generate-with>
    
	<source path="client" />
	<super-source path="junit"/>
</module>
