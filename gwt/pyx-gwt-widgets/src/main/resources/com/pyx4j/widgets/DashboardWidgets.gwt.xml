<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.pyx4j.log4gwt.Log4gwt'/>
    <inherits name='com.pyx4j.Commons'/>
    <inherits name='com.pyx4j.gwt.GwtCommons'/>
    <inherits name='com.pyx4j.gwt.GwtShared'/>
    <inherits name='com.pyx4j.i18n.I18N'/>
    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
    <inherits name='com.google.gwt.xml.XML' />

    <source path="client/dashboard" />

    <extend-configuration-property name="rpc.blacklist" value="com.pyx4j.widgets.client.*" />
    <extend-configuration-property name="rpc.blacklist" value="com.allen_sauer.gwt.dnd.*" />
</module>