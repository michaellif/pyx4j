<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<module>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.pyx4j.widgets.Widgets'/>
    <inherits name='com.pyx4j.log4gwt.Log4gwt'/>
    <inherits name='com.pyx4j.Commons'/>
    <inherits name='com.pyx4j.gwt.GwtClient'/>
    <inherits name='com.pyx4j.entity.Entity'/>
    <inherits name='com.pyx4j.entity.EntityRPC'/>

	<source path="client">
	   <exclude name="**/svg/**" />
    </source>

	<extend-configuration-property name="rpc.blacklist" value="com.pyx4j.forms.client.*" />
</module>
