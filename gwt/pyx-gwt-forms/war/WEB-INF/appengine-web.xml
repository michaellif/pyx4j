<?xml version="1.0" encoding="ISO-8859-1"?>
<appengine-web-app
    xmlns="http://appengine.google.com/ns/1.0">
    <!-- @version $Revision$ ($Author$) $Date$ -->
    <application>pyx4j-tester</application>
    <version>30</version>

    <precompilation-enabled>true</precompilation-enabled>
    <sessions-enabled>false</sessions-enabled>

    <static-files>
        <include
            path="**.nocache.*"
            expiration="1s" />
        <include
            path="/**.cache.*"
            expiration="1d" />
        <include
            path="/**.png"
            expiration="1d" />
        <include
            path="/**.gif"
            expiration="1d" />
        <include
            path="/test.html"
            expiration="1s" />
        <include
            path="/**/hosted.html"
            expiration="1s" />
    </static-files>

</appengine-web-app>