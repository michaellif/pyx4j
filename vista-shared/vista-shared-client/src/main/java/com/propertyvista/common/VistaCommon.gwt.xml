<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision$ ($Author$) $Date$ -->
<module>
    <inherits name='com.google.gwt.user.User'/>

    <inherits name='com.pyx4j.essentials.Essentials' />      
    <inherits name='com.pyx4j.security.RPC' />
    <inherits name='com.pyx4j.entity.Entity'/>
    <inherits name='com.pyx4j.rpc.RPC' />
    
    <inherits name='com.pyx4j.Commons' />
	<inherits name='com.pyx4j.site.GwtSite' />

    <inherits name='com.propertyvista.Domain' />
	<inherits name='com.propertyvista.portal.Rpc' />
    
    <source path="client"/>
    
    <replace-with class="com.pyx4j.config.client.ClientApplicationBackendConfigRDB">
        <when-type-is class="com.pyx4j.config.shared.ApplicationBackend.ApplicationBackendConfig"/>
    </replace-with>
    
    <extend-property name="locale" values="en_US"/>
    <set-property name="locale" value="en_US"/>
    <set-property-fallback name="locale" value="en_US"/>
    <set-configuration-property name="locale.cookie" value="locale"/>
    <set-configuration-property name="pyx.locale.default" value="en_US"/>
    
    <extend-configuration-property name="rpc.blacklist" value="com.propertyvista.common.client.*" />
    
</module>