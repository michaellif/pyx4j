<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision$ ($Author$) $Date$ -->
<module>

    <extend-property name="locale" values="en_CA"/>
    <extend-property name="locale" values="en_GB"/>
    <extend-property name="locale" values="fr"/>
    <extend-property name="locale" values="es"/>
    <extend-property name="locale" values="zh_CN"/>
    <extend-property name="locale" values="zh_TW"/>

    <set-property name="locale" value="en_US,en_CA,en_GB,fr,es,zh_CN,zh_TW"/>
    <set-property-fallback name="locale" value="en_CA"/>

</module>