<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module>

    <generate-with class="com.pyx4j.slf4j.rebind.LoggerFactoryGenerator">
        <when-type-assignable class="org.slf4j.ILoggerFactory"/>
    </generate-with>
    <define-configuration-property name="org.slf4j.ILoggerFactory" is-multi-valued="false" />

	<super-source path="slf4j-api-gwt"/>

	<extend-configuration-property name="rpc.blacklist" value="org.slf4j.helpers.*" />
</module>