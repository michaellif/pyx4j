    <inherits name='com.pyx4j.entity.test.EntitySharedTests'/>
    <inherits name='com.pyx4j.rpc.test.RPCTests'/>
	
	<source path="client"/>
	
	<entry-point class='com.pyx4j.tester.client.TesterClientEntryPoint'/>
	
	<extend-configuration-property name="rpc.blacklist" value="java.util.Stack" />
