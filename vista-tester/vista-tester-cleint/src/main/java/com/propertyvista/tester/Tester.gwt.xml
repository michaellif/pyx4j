<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision$ ($Author$) $Date$ -->
<module rename-to='tester'>
    
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.pyx4j.essentials.Essentials' />
    <inherits name='com.pyx4j.Commons' />
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name='com.pyx4j.site.GwtSite' />

    <inherits name='com.propertyvista.portal.test.DomainTests' />
    <inherits name='com.propertyvista.portal.test.RpcTests' />
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    
    <inherits name='com.propertyvista.common.VistaCommon' />
    
    <stylesheet src="vista-tester.css" />
    
    <source path="client"/>

   <entry-point class='com.propertyvista.tester.client.TesterSite'/>
    
   
   <extend-configuration-property name="rpc.blacklist" value="com.propertyvista.tester.client.*" />
    
</module>