<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="LeaseTermSignatures" pageWidth="450" pageHeight="792" columnWidth="450" leftMargin="0" rightMargin="0" topMargin="5" bottomMargin="10">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<field name="signatureFormat" class="com.pyx4j.entity.shared.ISignature.SignatureFormat"/>
	<field name="fullName" class="java.lang.String"/>
	<field name="initials" class="java.lang.String"/>
	<field name="agree" class="java.lang.Boolean"/>
	<field name="signDate" class="java.util.Date"/>
	<field name="ipAddress" class="java.lang.String"/>
	<detail>
		<band height="44" splitType="Stretch">
			<printWhenExpression><![CDATA[$F{signatureFormat} == com.pyx4j.entity.shared.ISignature.SignatureFormat.FullName || $F{signatureFormat} == com.pyx4j.entity.shared.ISignature.SignatureFormat.AgreeBoxAndFullName]]></printWhenExpression>
			<staticText>
				<reportElement x="0" y="1" width="182" height="20"/>
				<textElement textAlignment="Right" verticalAlignment="Bottom">
					<font isBold="true" isUnderline="false"/>
				</textElement>
				<text><![CDATA[Name:]]></text>
			</staticText>
			<textField isStretchWithOverflow="true">
				<reportElement stretchType="RelativeToBandHeight" x="0" y="21" width="182" height="20"/>
				<textElement textAlignment="Right" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{fullName}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement x="307" y="1" width="143" height="20"/>
				<textElement verticalAlignment="Bottom">
					<font isBold="true" isUnderline="false"/>
				</textElement>
				<text><![CDATA[Date:]]></text>
			</staticText>
			<textField>
				<reportElement x="194" y="21" width="113" height="20"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{ipAddress}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement x="194" y="1" width="113" height="20"/>
				<textElement verticalAlignment="Bottom">
					<font isBold="true" isUnderline="false"/>
				</textElement>
				<text><![CDATA[IP:]]></text>
			</staticText>
			<textField pattern="d MMM yyyy HH:mm:ss">
				<reportElement x="307" y="21" width="143" height="20"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{signDate}]]></textFieldExpression>
			</textField>
		</band>
		<band height="15">
			<printWhenExpression><![CDATA[$F{signatureFormat} == com.pyx4j.entity.shared.ISignature.SignatureFormat.Initials]]></printWhenExpression>
			<textField isStretchWithOverflow="true">
				<reportElement stretchType="RelativeToBandHeight" x="0" y="0" width="450" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA["I Agree to the terms above (" + $F{initials} +")"]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
