<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="LeaseTermSignatures" pageWidth="468" pageHeight="792" columnWidth="468" leftMargin="0" rightMargin="0" topMargin="5" bottomMargin="10">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<field name="signatureFormat" class="com.pyx4j.entity.shared.ISignature.SignatureFormat"/>
	<field name="fullName" class="java.lang.String"/>
	<field name="initials" class="java.lang.String"/>
	<field name="agree" class="java.lang.Boolean"/>
	<field name="signDate" class="java.util.Date"/>
	<field name="ipAddress" class="java.lang.String"/>
	<detail>
		<band height="35" splitType="Stretch">
			<printWhenExpression><![CDATA[$F{signatureFormat} == com.pyx4j.entity.shared.ISignature.SignatureFormat.FullName || $F{signatureFormat} == com.pyx4j.entity.shared.ISignature.SignatureFormat.AgreeBoxAndFullName]]></printWhenExpression>
			<staticText>
				<reportElement x="0" y="1" width="171" height="14"/>
				<textElement verticalAlignment="Bottom">
					<font isBold="true" isUnderline="true"/>
				</textElement>
				<text><![CDATA[Tenant:]]></text>
			</staticText>
			<textField>
				<reportElement x="0" y="15" width="171" height="20"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{fullName}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement x="309" y="1" width="159" height="14"/>
				<textElement verticalAlignment="Bottom">
					<font isBold="true" isUnderline="true"/>
				</textElement>
				<text><![CDATA[Date:]]></text>
			</staticText>
			<textField>
				<reportElement x="171" y="15" width="138" height="20"/>
				<textElement/>
				<textFieldExpression><![CDATA[$F{ipAddress}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement x="171" y="1" width="138" height="14"/>
				<textElement verticalAlignment="Bottom">
					<font isBold="true" isUnderline="true"/>
				</textElement>
				<text><![CDATA[IP:]]></text>
			</staticText>
			<textField pattern="MMMMM dd, yyyy">
				<reportElement x="309" y="15" width="159" height="20"/>
				<textElement/>
				<textFieldExpression><![CDATA[$F{signDate}]]></textFieldExpression>
			</textField>
		</band>
		<band height="16">
			<printWhenExpression><![CDATA[$F{signatureFormat} == com.pyx4j.entity.shared.ISignature.SignatureFormat.Initials]]></printWhenExpression>
			<textField>
				<reportElement x="0" y="0" width="468" height="15"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA["I Agree to the terms above (" + $F{initials} +")"]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
