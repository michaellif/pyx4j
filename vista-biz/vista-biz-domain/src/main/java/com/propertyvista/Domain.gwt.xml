<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<!-- @version $Revision$ ($Author$) $Date$ -->
<module>
    <inherits name='com.pyx4j.entity.EntityShared'/>
   
    <source path="domain"/>
    <source path="dto"/>
    <source path="misc"/>
    <source path="shared"/>
    
    <replace-with class="com.propertyvista.common.client.config.VistaFeaturesCustomizationClient">
         <when-type-is class="com.propertyvista.shared.config.VistaFeatures.VistaFeaturesCustomization"/>
    </replace-with>

    <replace-with class="com.propertyvista.common.client.config.demo.VistaDemoConfigurationDefault">
         <when-type-is class="com.propertyvista.shared.config.VistaDemo.VistaDemoConfiguration"/>
    </replace-with>
    
    <extend-configuration-property name="rpc.blacklist" value="com.propertyvista.domain.security.common.AbstractUserCredential" />
    
</module>