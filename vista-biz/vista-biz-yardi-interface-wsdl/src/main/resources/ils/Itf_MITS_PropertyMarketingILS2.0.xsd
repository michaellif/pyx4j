<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:MITS="http://my-company.com/namespace" elementFormDefault="qualified" attributeFormDefault="unqualified" >
	<xs:import namespace="http://my-company.com/namespace" schemaLocation="./Itf_MITS_CoreData2.0.xsd"/>
	<xs:element name="PhysicalProperty">
		<xs:annotation>
			<xs:documentation>MITS Property v2.0 Final DRAFT updated 1/23/07</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Management" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Id</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PropertyContacts" type="MITS:PropertyContactsType">
								<xs:annotation>
									<xs:documentation>MITS PropertyContacts Core Data module version 2.0</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="File" type="FileType" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Id, Active</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="Id" use="optional">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Property" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>managementID</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PropertyID" type="MITS:PropertyIDType">
								<xs:annotation>
									<xs:documentation>MITS PropertyID Core Data Module version 2.0</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="General_ID" type="GeneralIDType" minOccurs="0" maxOccurs="10"/>
							<xs:element name="ILS_Identification">
								<xs:annotation>
									<xs:documentation>Type, RentalType</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Latitude" type="xs:decimal" minOccurs="0"/>
										<xs:element name="Longitude" type="xs:decimal" minOccurs="0"/>
										<xs:element name="DaylightSaving" type="xs:boolean" minOccurs="0"/>
										<xs:element name="TimeZone" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="LastUpdate" minOccurs="0">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="Type" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Apartment"/>
												<xs:enumeration value="Condo"/>
												<xs:enumeration value="Duplex"/>
												<xs:enumeration value="Triplex"/>
												<xs:enumeration value="4plex"/>
												<xs:enumeration value="Senior"/>
												<xs:enumeration value="Assisted Living"/>
												<xs:enumeration value="Subsidized"/>
												<xs:enumeration value="High Rise"/>
												<xs:enumeration value="Garden Style"/>
												<xs:enumeration value="Mid Rise"/>
												<xs:enumeration value="Mixed Use"/>
												<xs:enumeration value="Townhouse"/>
												<xs:enumeration value="Vacation"/>
												<xs:enumeration value="Campus"/>
												<xs:enumeration value="Military"/>
												<xs:enumeration value="House for Rent"/>
												<xs:enumeration value="Corporate"/>
												<xs:enumeration value="Unspecified"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="RentalType" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Subsidized"/>
												<xs:enumeration value="Assisted Senior"/>
												<xs:enumeration value="Assisted Living"/>
												<xs:enumeration value="Market Rate"/>
												<xs:enumeration value="Unspecified"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Information">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="StructureType">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Standard"/>
													<xs:enumeration value="High Rise"/>
													<xs:enumeration value="Garden Style"/>
													<xs:enumeration value="Mid Rise"/>
													<xs:enumeration value="Mixed Use"/>
													<xs:enumeration value="Unspecified"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="BuildingCount" type="xs:integer" minOccurs="0"/>
										<xs:element name="UnitCount" type="xs:integer"/>
										<xs:element name="NumberOfAcres" type="xs:decimal" minOccurs="0"/>
										<xs:element name="YearBuilt" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="MITS-Year"/>
											</xs:simpleType>
										</xs:element>
										<xs:element name="YearRemodeled" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="MITS-Year"/>
											</xs:simpleType>
										</xs:element>
										<xs:element name="AcquisitionDate" minOccurs="0">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="OfficeHour" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="OpenTime">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="50"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="CloseTime">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="50"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Day">
														<xs:annotation>
															<xs:documentation>Days open: Su, M, T, W, Th, F, Sa </xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="20"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ShortDescription" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="500"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="LongDescription" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="1000"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="LeaseLength" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Rents" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SalePrice" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="StartRent" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="StandardRent" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="CorporateRent" minOccurs="0">
														<xs:complexType>
															<xs:attribute name="Min" type="MITS-Currency" use="optional"/>
															<xs:attribute name="Max" type="MITS-Currency" use="optional"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="IncomeRentQual" minOccurs="0" maxOccurs="6">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="IncomeLimit" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="QualifiedRent" type="MITS-Currency" minOccurs="0"/>
												</xs:sequence>
												<xs:attribute name="Persons" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:integer">
															<xs:enumeration value="1"/>
															<xs:enumeration value="2"/>
															<xs:enumeration value="3"/>
															<xs:enumeration value="4"/>
															<xs:enumeration value="5"/>
															<xs:enumeration value="6"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="Parking" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Type</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Assigned" type="xs:boolean" minOccurs="0"/>
													<xs:element name="AssignedFee" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="Free"/>
																<xs:enumeration value="Paid"/>
																<xs:enumeration value="Both"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="SpaceFee" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="Spaces" type="xs:integer" minOccurs="0"/>
													<xs:element name="Comment" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="500"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="Type" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Surface Lot"/>
															<xs:enumeration value="Garage Lot"/>
															<xs:enumeration value="Covered Lot"/>
															<xs:enumeration value="Street"/>
															<xs:enumeration value="Other"/>
															<xs:enumeration value="None"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="DrivingDirections" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="1000"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Services" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Nearest</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Name">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="200"/>
																<xs:enumeration value="Bus"/>
																<xs:enumeration value="Elementary School"/>
																<xs:enumeration value="Middle School"/>
																<xs:enumeration value="High School"/>
																<xs:enumeration value="Highway"/>
																<xs:enumeration value="Laundromat"/>
																<xs:enumeration value="Drycleaner"/>
																<xs:enumeration value="Park"/>
																<xs:enumeration value="Train"/>
																<xs:enumeration value="Other"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Detail" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="500"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="DistanceTo" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="100"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Comment" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="500"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="Nearest" type="xs:boolean" use="required"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="PropertyAvailabilityURL" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="250"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Accounting" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="LiveDate">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="LiveFlag" type="xs:boolean"/>
										<xs:element name="LastDayBegin">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="LastDayEnd">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="LastAccMthEnd">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="AcctMthEndFlag">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Preliminary"/>
													<xs:enumeration value="Changes"/>
													<xs:enumeration value="Month Closed"/>
													<xs:enumeration value="None"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="LastInvoiceNumber" default="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="40"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="LastReceiptNumber">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="40"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="CashReserve" type="MITS-Currency"/>
										<xs:element name="EndOfYear">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="AccountsReceivable">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="AccountsPayable">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="GeneralLedger">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="CurrentAccountingPeriod">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="CurrentAccountingPeriodAR">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="CurrentAccountingPeriodAP">
											<xs:complexType>
												<xs:attributeGroup ref="MITS-DateGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="TaxCredit">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Payment" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="RemitTo" type="xs:string"/>
										<xs:element name="CheckPayable">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Fee" minOccurs="0">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="FeeType">
											<xs:sequence>
												<xs:element name="AdminFee" type="MITS-Currency"/>
												<xs:element name="ApplicationFee" type="MITS-Currency"/>
											</xs:sequence>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="Concession" minOccurs="0" maxOccurs="10">
								<xs:annotation>
									<xs:documentation>Active</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="ConcessionType"/>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="Amenity" type="GeneralAmenityType" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="Deposit" type="DepositType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Type</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Policy" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Pet" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Type, Allowed</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Comment" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="500"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="MaxCount" type="xs:integer" minOccurs="0"/>
													<xs:element name="Deposit" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="Fee" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="Rent" type="MITS-Currency" minOccurs="0"/>
													<xs:element name="Weight" type="xs:decimal" minOccurs="0"/>
													<xs:element name="FeeRange" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Min, Max </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="Min" type="MITS-Currency" use="optional"/>
															<xs:attribute name="Max" type="MITS-Currency" use="optional"/>
														</xs:complexType>
													</xs:element>
													<xs:element name="Restrictions" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="500"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="PetCare" type="xs:boolean" minOccurs="0"/>
												</xs:sequence>
												<xs:attribute name="Type" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="Allowed" type="xs:boolean" use="required"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="General" minOccurs="0">
											<xs:annotation>
												<xs:documentation>General Policy</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="500"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Phase" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Id, BuildingID</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Name">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Description">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="1000"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="UnitCount" type="xs:integer"/>
										<xs:element name="RentableUnits" type="xs:integer"/>
										<xs:element name="TotalSquareFeet" type="xs:integer"/>
										<xs:element name="RentableSquareFeet" type="xs:integer"/>
										<xs:element name="Amenity" type="GeneralAmenityType" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="File" type="FileType" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Id, Active</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Promotional" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Promotional Bullet Points for ILS Listings</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="500"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="Id" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
