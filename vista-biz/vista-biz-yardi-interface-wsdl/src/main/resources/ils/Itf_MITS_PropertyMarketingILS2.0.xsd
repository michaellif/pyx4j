		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="MITS-Integer">
		<xs:annotation>
			<xs:documentation>Standard MITS Integer format</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(\d*)?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MITS-Boolean">
		<xs:annotation>
			<xs:documentation>Standard MITS Boolean format</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(true)?|(false)?|(0)?|(1)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MITS-Decimal">
		<xs:annotation>
			<xs:documentation>Standard MITS Decimal format</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="-?(\d*)?\.?(\d*)?"/>
		</xs:restriction>
